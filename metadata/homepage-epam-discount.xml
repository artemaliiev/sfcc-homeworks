<?xml version="1.0" encoding="UTF-8"?>
<metadata xmlns="http://www.demandware.com/xml/impex/metadata/2006-10-31">

    <type-extension type-id="SitePreferences">
        <custom-attribute-definitions>
            <attribute-definition attribute-id="GoogleVerificationTag">
                <display-name xml:lang="x-default">GoogleVerificationTag - Content Attribute</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="TestBackground">
                <display-name xml:lang="x-default">Test Background Color</display-name>
                <description xml:lang="x-default">Hexadecimal color value</description>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="checkTLS">
                <display-name xml:lang="x-default">Check TLS</display-name>
                <description xml:lang="x-default">When true, this causes a TLS check to take place based on whether or not a cookie ('dw_TLSWarning') has been set on the browser</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="countryCode">
                <display-name xml:lang="x-default">Default Country Code</display-name>
                <description xml:lang="x-default">Default country code for store searches</description>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition default="true">
                        <display xml:lang="x-default">USA</display>
                        <value>US</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">CANADA</display>
                        <value>CA</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="customerServiceEmail">
                <display-name xml:lang="x-default">Customer Service Email</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <field-length>0</field-length>
                <default-value>no-reply@testorganization.com</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="disableResponsiveDesign">
                <display-name xml:lang="x-default">Disable Responsive Design</display-name>
                <description xml:lang="x-default">Disable Responsive Design for Storefront</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="enableInfiniteScroll">
                <display-name xml:lang="x-default">Enable Infinite Scroll</display-name>
                <description xml:lang="x-default">This Boolean toggles the option for infinite scrolling or pagination from within the search grid.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="enableMultiShipping">
                <display-name xml:lang="x-default">Enable MultiShipping</display-name>
                <description xml:lang="x-default">The attribute is used to enable/disable Multi-Shipment Checkout for each site via BM.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="homepageEpamDiscount">
                <display-name xml:lang="x-default">Special discount</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="listPriceDefault">
                <display-name xml:lang="x-default">Default List Price Book ID</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
                <default-value>usd-m-list-prices</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="mapAPI">
                <display-name xml:lang="x-default">google map api key</display-name>
                <type>string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-length>0</min-length>
            </attribute-definition>
            <attribute-definition attribute-id="rateLimiterThreshold">
                <display-name xml:lang="x-default">Rate Limiter Threshold</display-name>
                <description xml:lang="x-default">This number indicates the number of times failures for Logins, GiftCert Balances, and Order Tracking requests will be allowed before displaying a CAPTCHA-style pop-up rate limiter</description>
                <type>int</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <min-value>0.0</min-value>
                <default-value>5</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="sfraEnableOverlayApplePay">
                <display-name xml:lang="x-default">SFRA - Enable Apple Pay</display-name>
                <description xml:lang="x-default">Enables the Apple Pay plugin in the SFRA Unified Feature Cartridge. Note: The cartridge plugin_applepay must be added to the cartridge path and uploaded to your instance.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="sfraEnableOverlayCommercePayments">
                <display-name xml:lang="x-default">SFRA - Enable Commerce Payments</display-name>
                <description xml:lang="x-default">Enables the Commerce Payments plugin in the SFRA Unified Feature Cartridge. Note: The cartridge plugin_commercepayments must be added to the cartridge path and uploaded to your instance.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>false</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="sfraEnableOverlayDataDownload">
                <display-name xml:lang="x-default">SFRA - Enable Data Download</display-name>
                <description xml:lang="x-default">Enables the Data Download plugin in the SFRA Unified Feature Cartridge. Note: The cartridge plugin_datadownload must be added to the cartridge path and uploaded to your instance.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="sfraEnableOverlayGiftRegistry">
                <display-name xml:lang="x-default">SFRA - Enable Gift Registry</display-name>
                <description xml:lang="x-default">Enables the Gift Registry plugin in the SFRA Unified Feature Cartridge. Note: The cartridge plugin_giftregistry must be added to the cartridge path and uploaded to your instance.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="sfraEnableOverlayInStorePickup">
                <display-name xml:lang="x-default">SFRA - Enable In Store Pickup</display-name>
                <description xml:lang="x-default">Enables the In Store Pickup Plugin in the SFRA Unified Feature Cartridge. Note: The plugin_instorepickup must be added to the cartridge path and uploaded to your instance.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="sfraEnableOverlayProductCompare">
                <display-name xml:lang="x-default">SFRA - Enable Product Compare</display-name>
                <description xml:lang="x-default">Enables the Product Compare plugin in the SFRA Unified Feature Cartridge. Note: The cartridge plugin_productcompare must be added to the cartridge path and uploaded to your instance.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="sfraEnableOverlayWishlist">
                <display-name xml:lang="x-default">SFRA - Enable Wishlist</display-name>
                <description xml:lang="x-default">Enables the Wishlist plugin in the SFRA Unified Feature Cartridge. Note: The cartridge plugin_wishlists must be added to the cartridge path and uploaded to your instance.</description>
                <type>boolean</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <default-value>true</default-value>
            </attribute-definition>
            <attribute-definition attribute-id="slideshoweffect">
                <display-name xml:lang="x-default">Slide Slow effect</display-name>
                <description xml:lang="x-default">Please visit http://jquery.malsup.com/cycle/browser.html for complete visual effects</description>
                <type>enum-of-string</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition>
                        <display xml:lang="x-default">blindX (slide from left)</display>
                        <value>blindX</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">blindY (slide from bottom)</display>
                        <value>blindY</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">blindZ (slide from bottom right corner)</display>
                        <value>blindZ</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">cover (slide from left, clean)</display>
                        <value>cover</value>
                    </value-definition>
                    <value-definition default="true">
                        <display xml:lang="x-default">curtainX (expand from center horizontally)</display>
                        <value>curtainX</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">curtainY (expand from center vertically)</display>
                        <value>curtainY</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">fade</display>
                        <value>fade</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">fadeZoom (Zoom out from center)</display>
                        <value>fadeZoom</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">growX (grow from center horizontally)</display>
                        <value>growX</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">growY (grow from center vertically)</display>
                        <value>growY</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">none (No effects)</display>
                        <value>none</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">scrollUp (Scroll Up)</display>
                        <value>scrollUp</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">scrollDown (Scroll Down)</display>
                        <value>scrollDown</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">scrollLeft (Scroll Left)</display>
                        <value>scrollLeft</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">scrollRight (Scroll Right)</display>
                        <value>scrollRight</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">scrollHorz (Scroll Horizontally)</display>
                        <value>scrollHorz</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">scrollVert (Scroll Vertically)</display>
                        <value>scrollVert</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">shuffle</display>
                        <value>shuffle</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">slideX (Slide to the right)</display>
                        <value>slideX</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">slideY (Slide Bottom)</display>
                        <value>slideY</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">toss</display>
                        <value>toss</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">turnUp</display>
                        <value>turnUp</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">turnDown</display>
                        <value>turnDown</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">turnLeft</display>
                        <value>turnLeft</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">turnRight</display>
                        <value>turnRight</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">uncover</display>
                        <value>uncover</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">wipe</display>
                        <value>wipe</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">zoom</display>
                        <value>zoom</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
            <attribute-definition attribute-id="storeLookupMaxDistance">
                <display-name xml:lang="x-default">Store Lookup Max Distance</display-name>
                <description xml:lang="x-default">Store Lookup Max Distance</description>
                <type>enum-of-int</type>
                <mandatory-flag>false</mandatory-flag>
                <externally-managed-flag>false</externally-managed-flag>
                <value-definitions>
                    <value-definition>
                        <display xml:lang="x-default">50</display>
                        <value>50</value>
                    </value-definition>
                    <value-definition default="true">
                        <display xml:lang="x-default">75</display>
                        <value>75</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">100</display>
                        <value>100</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">150</display>
                        <value>150</value>
                    </value-definition>
                    <value-definition>
                        <display xml:lang="x-default">200</display>
                        <value>200</value>
                    </value-definition>
                </value-definitions>
            </attribute-definition>
        </custom-attribute-definitions>
        <group-definitions>
            <attribute-group group-id="Storefront Configs">
                <display-name xml:lang="x-default">Storefront Configurations</display-name>
                <attribute attribute-id="listPriceDefault"/>
                <attribute attribute-id="GoogleVerificationTag"/>
                <attribute attribute-id="countryCode"/>
                <attribute attribute-id="customerServiceEmail"/>
                <attribute attribute-id="mapAPI"/>
            </attribute-group>
            <attribute-group group-id="SFRA Unified Feature Cartridge">
                <display-name xml:lang="x-default">SFRA Unified Feature Cartridge</display-name>
                <attribute attribute-id="sfraEnableOverlayInStorePickup"/>
                <attribute attribute-id="sfraEnableOverlayDataDownload"/>
                <attribute attribute-id="sfraEnableOverlayWishlist"/>
                <attribute attribute-id="sfraEnableOverlayGiftRegistry"/>
                <attribute attribute-id="sfraEnableOverlayCommercePayments"/>
                <attribute attribute-id="sfraEnableOverlayApplePay"/>
                <attribute attribute-id="sfraEnableOverlayProductCompare"/>
            </attribute-group>
            <attribute-group group-id="EpamDiscount">
                <display-name xml:lang="x-default">Discount Message</display-name>
                <attribute attribute-id="homepageEpamDiscount"/>
            </attribute-group>
            <attribute-group group-id="Test Config">
                <display-name xml:lang="x-default">Test Config</display-name>
                <attribute attribute-id="TestBackground"/>
            </attribute-group>
        </group-definitions>
    </type-extension>

</metadata>
